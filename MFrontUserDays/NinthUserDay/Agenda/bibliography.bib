@article{chen_microplane_2014,
	title = {Microplane damage model for jointed rock masses: {MICROPLANE} {DAMAGE} {MODEL} {FOR} {JOINTED} {ROCK} {MASSES}},
	volume = {38},
	issn = {03639061},
	shorttitle = {Microplane damage model for jointed rock masses},
	url = {http://doi.wiley.com/10.1002/nag.2257},
	doi = {10.1002/nag.2257},
	abstract = {The paper presents a new microplane constitutive model for the inelastic behavior of jointed rock masses that takes into account the mechanical behavior and geometric characteristics of cracks and joints. The basic idea is that the microplane modeling of rock masses under general triaxial loading, including compression, requires the isotropic rock matrix and the joints to be considered as two distinct phases coupled in parallel. A joint continuity factor is deﬁned as a microplane damage variable to represent the stress-carrying area fraction of the joint phase. Based on the assumption of parallel coupling between the rock joint and the rock matrix, the overall mechanical behavior of the rock is characterized by microplane constitutive laws for the rock matrix and for the rock joints, along with an evolution law for the microplane joint continuity factor. The inelastic response of the rock matrix and the rock joints is controlled on the microplane level by the stress–strain boundaries. Based on the arguments enunciated in developing the new microplane model M7 for concrete, the previously used volumetric–deviatoric splits of the elastic strains and of the tensile boundary are avoided. The boundaries are tensile normal, compressive normal, and shear. The numerical simulations demonstrate satisfactory ﬁts of published triaxial test data on sandstone and on jointed plaster mortar, including quintessential features such as the strain softening and dilatancy under low conﬁning pressure, as well as the brittle–ductile transition under higher conﬁning pressure, and the decrease of jointed rock strength and Young’s modulus with an increasing dip angle of the joint. Copyright © 2014 John Wiley \& Sons, Ltd.},
	language = {en},
	number = {14},
	urldate = {2020-09-26},
	journal = {International Journal for Numerical and Analytical Methods in Geomechanics},
	author = {Chen, Xin and Bažant, Zdeněk P.},
	month = oct,
	year = {2014},
	pages = {1431--1452},
	file = {Chen and Bažant - 2014 - Microplane damage model for jointed rock masses M.pdf:/Users/brenonogueira/Zotero/storage/NKFSLFXK/Chen and Bažant - 2014 - Microplane damage model for jointed rock masses M.pdf:application/pdf}
}

@article{steinke_relation_2017,
